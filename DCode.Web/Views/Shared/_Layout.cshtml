        function calWindowSize() {
            var headerHeight = 0;
            if (document.querySelector(".right-body-top") != null)
                headerHeight = document.querySelector(".right-body-top").clientHeight;
            var windowHeight = window.innerHeight;
            var projectContainer_h = windowHeight - headerHeight - 190  + 'px';
            console.log(projectContainer_h);
            if (document.querySelector(".right-container") != null)
                document.querySelector(".right-container").style.height = projectContainer_h;
        }
        calWindowSize();
        window.onresize = function () {
            calWindowSize();
            document.querySelector(".right-container").style.height = window.innerHeight - document.querySelector(".right-body-top").clientHeight + 'px';
        }
        $(document).ready(function () {

            $("#datetimepicker1").datetimepicker({
                minDate: new Date(),
                useCurrent: false,
                format: 'MM/DD/YYYY'
            });
            $("#datetimepicker2").datetimepicker({
                minDate: new Date(),
                useCurrent: false,
                format: 'MM/DD/YYYY'
            });
            
            $('.nav li a').click(function (e) {
                $('.nav li').removeClass('active');
                var $parent = $(this).parent();
                if (!$parent.hasClass('active')) {
                    $parent.addClass('active');
                }
                e.preventDefault();
            });

        });


    </script>

    <script type="text/javascript">

