<div class="new-style-sai-main-inner-body">
    <div id="approvalContainer" style="padding:20px;" ng-show="dashboard.showApproval">
        <div @*id="divAssigned"*@ class="success-banner" ng-show="AssignedDivModel.Visible">
            <h3 class="dark-grey-blue font-size-24"><span class="title-new">{{AssignedDivModel.AssigneeName}} has been assigned {{AssignedDivModel.AssignedTask}} for {{AssignedDivModel.AssignedHours}} starting {{AssignedDivModel.StartingDate}}</span></h3>
        </div>

        <div class="background-color-ecf0f1" infinite-scroll='getApplicants()' infinite-scroll-distance='0'>
            <div ng-show="taskApplicants.length == 0" style="padding-top:20px;">
                <div class="new-style-sai-new-task-background">
                    <div>
                        <div class="dark-grey-blue font-size-24">Want to create more tasks?</div>
                    </div>
                    <div> &nbsp;</div>
                    <div>
                        <div class="new-style-sai-create-btn-side">&nbsp;</div>
                        <div class="new-style-sai-create-new-task-btn new-style-sai-create-btn-center" ng-click="NavigateToNewTask()">CREATE NEW TASK</div>
                        <div class="new-style-sai-create-btn-side-bottom">&nbsp;</div>

                    </div>
                </div>
            </div>
            <div class="new-style-sai-visibility">
                &nbsp;
            </div>
            <div class="project-details" style="padding:20px;" ng-repeat="taskApplicant in taskApplicants track by $index">
                <div>
                    <div ng-hide="taskApplicant.Task.Status == 'Assigned'">
                        <div style="width:100%;">
                            <div style="float:left;">
                                <div class="heading" style="color: #8e44ad;">{{taskApplicant.Task.ProjectName}}</div>
                                <div class="gray-text">{{taskApplicant.Task.Duration}}</div>
                            </div>
                            <div style="float:right">
                                <div class="circle-txt-60">{{taskApplicant.Task.ShortName}}</div>
                            </div>
                        </div>
                        <div style="clear:both;"></div>
                        <div style="width:100%;text-align:left;" class="heading">
                            <div class="new-style-sai-tracking-main">
                                <span class="skill-icon"><img src="/Content/Images/skill@2x.png" /></span>
                                <span class="role-desc dark-grey-blue">{{taskApplicant.Task.Skills}}</span>
                                <div class="gray-text">Required skills</div>
                            </div>
                            <div class="new-style-sai-tracking-main">
                                <span class="hours-icon"><img src="/Content/Images/hours@2x.png" /></span>
                                <span class="role-desc dark-grey-blue">{{taskApplicant.Task.Hours}} hours</span>
                                <div class="gray-text">Billable hours</div>
                            </div>
                            <div class="new-style-sai-tracking-main">
                                <span class="cal-icon"><img src="/Content/Images/calendar@2x.png" /></span>
                                <span class="role-desc dark-grey-blue">{{taskApplicant.Task.OnBoardingDate}}</span>
                                <div class="gray-text">Onboarding Date</div>
                            </div>
                            <div class="new-style-sai-tracking-main">
                                <span class="gift-icon"><img src="/Content/Images/serviceLine.png" /></span>
                                <span class="role-desc dark-grey-blue">{{taskApplicant.Task.ServiceLine}}</span>
                                <div class="gray-text">Service Line</div>
                            </div>

                        </div>
                        <div style="clear:both;"></div>
                        <div style="width:100%;padding-top:30px;">
                            <div style="width:65%;float:left;">
                                <p class="project-desc">{{taskApplicant.Task.Details}}</p>
                            </div>
                            <div style="width:35%;float:right;">
                                <div id="responseDiv_$index" class="new-style-sai-response-txt" ng-click="showStatusResponse($index);">
                                    {{taskApplicant.Applicants.length}} Response<span ng-show="taskApplicant.Applicants.length > 1 || taskApplicant.Applicants.length == 0">s</span>
                                    <span ng-switch="isShowing($index)" ng-show="taskApplicant.Applicants.length > 0">
                                        <span ng-switch-when=false>
                                            <img src="/Content/Images/downward@2x.png" width="8px" height="8px" />
                                        </span>
                                        <span ng-switch-when=true>
                                            <img src="/Content/Images/upward@2x.png" width="8px" height="8px" />
                                        </span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div style="clear:both;"></div>

                        @*<div>
                                &nbsp;
                            </div>*@
                    </div>
                    <div ng-show="isShowing($index)">
                        <div style="width:100%;text-align:center;" ng-repeat="applicant in taskApplicant.Applicants" ng-hide="taskApplicant.Task.Status == 'Assigned'">

                            <div @*class="new-style-sai-20-div"*@>
                                <div class="circle-txt-60">{{applicant.ShortName}}</div>
                            </div>
                            <div class="new-style-sai-20-div">
                                <div class="font-size-20 color-8e44ad">{{applicant.Name}}</div>
                                <div class="font-size-14">{{applicant.Designation}}</div>
                            </div>
                            <div class="new-style-sai-20-div">
                                <div class="font-size-20">{{applicant.Expertise}}</div>
                                <div class="font-size-14">Skills</div>
                            </div>
                            @*<div class="new-style-sai-20-div">
                                    <div>
                                        <div class="happy-icon" style="float:left;">
                                            <img src="/Content/Images/happy@2x.png" class="col-md-6 no-padding-left-right" />
                                        </div>
                                        <div style="padding-top:5px;" class="Recommended-number">{{applicant.TopRatingsCount}}</div>
                                    </div>
                                    <div style="clear:both;"></div>
                                    <div class="mgr-deg link">Recommended by </div>
                                </div>*@
                            <div class="new-style-sai-20-div">
                                <div class="add-task-button-sai-disabled" ng-show="taskApplicant.Task.Status == 'Assigned'">Assign</div>
                                <div class="add-task-button-sai" ng-hide="taskApplicant.Task.Status == 'Assigned'" ng-click="assignTaskToApplicant(taskApplicant.Task , applicant, $index,taskApplicant.TaskApplicantId)">ASSIGN</div>
                            </div>
                            <div>
                                &nbsp;
                            </div>
                        </div>

                        @*<div class="background-color-fff" ng-repeat="applicant in taskApplicant.Applicants" ng-hide="taskApplicant.Task.Status == 'Assigned'">
                                <div class="col-md-12 col-sm-12 project-details-expand margin-top-bottom-1 background-color-fff padding-left-right-20">
                                    <div class="col-md-12 border-bottom-dark no-padding">
                                        <div class="col-md-3 no-padding">
                                            <div class="col-sm-12 col-md-12 no-padding-left-right">
                                                <div class="pull-left col-md-4 no-padding-left-right">
                                                    <div class="circle-txt-60 pull-left margin-top-10">{{applicant.ShortName}}</div>
                                                </div>
                                                <div class="pull-left col-sm-8 col-md-8 margin-top-bottom-20 no-margin-left-right no-padding-left-right">
                                                    <div class="pull-left mgr-name col-md-12 row font-size-20 color-8e44ad">{{applicant.Name}}</div>
                                                    <div class="pull-left mgr-deg col-md-12 row font-size-14">{{applicant.Designation}}</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 no-padding">
                                            <div class="pull-left col-sm-12 col-md-12 margin-top-bottom-20 no-margin-left-right no-padding-left-right">
                                                <div class="pull-left mgr-name col-md-12 row font-size-20">{{applicant.Expertise}}</div>
                                                <div class="pull-left mgr-deg col-md-12 row font-size-14">Skills</div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 margin-top-16">
                                            <div class="col-md-12 ">
                                                <div class="col-md-3 no-padding-left-right happy-icon">
                                                    <img src="/Content/Images/happy@2x.png" class="col-md-6 no-padding-left-right" />
                                                </div>
                                                <div class="col-md-4 font-size-20 no-padding-left-right Recommended-number">{{applicant.TopRatingsCount}}</div>
                                            </div>
                                            <div class="col-md-12 font-size-14 mgr-deg link">Recommended by </div>
                                        </div>
                                        <div class="col-md-2 col-md-offset-1">
                                            <div class="add-task-button-disabled margin-top-24" ng-show="taskApplicant.Task.Status == 'Assigned'">Assign</div>
                                            <div class="add-task-button margin-top-24" ng-hide="taskApplicant.Task.Status == 'Assigned'" ng-click="assignTaskToApplicant(taskApplicant.Task , applicant, $index,taskApplicant.TaskApplicantId)">ASSIGN</div>
                                        </div>
                                    </div>
                                </div>
                            </div>*@

                    </div>
                </div>
                <div>
                    <hr style="background:darkgray;" />
                </div>
            </div>

        </div>
    </div>
</div>



